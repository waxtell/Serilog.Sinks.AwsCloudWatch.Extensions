{
  "AWS": {
    "Profile": "Default",
    "Region": "us-east-2",
    "AWSProfilesLocation": "aws_credentials.txt"
  },
  "Serilog": {
    "Using": [
      "Serilog.Settings.Configuration",
      "Serilog.Sinks.AwsCloudWatch.Extensions"
    ],
    "MinimumLevel": {
      "Default": "Verbose"
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "CloudWatch",
        "Args": {
          "logGroupName": "SampleApp9000",
          "regionEndpointSystemName": "us-east-2"
        }
      }
    ]
  }
}